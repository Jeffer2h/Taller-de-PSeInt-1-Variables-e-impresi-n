Proceso cuarto_punto
	Definir pais Como Caracter;
	Definir capital Como Caracter;
	
	Escribir "Ingrese el nombre de un país: ";
	Leer pais;
	Escribir "Ingrese el nombre de la capital del país ingresado: ";
	Leer capital;
	
	Escribir "La ciudad ",capital, ", es la capital del país ", pais;
	
FinProceso
